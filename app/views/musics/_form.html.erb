<%= form_for(@music) do |f| %>

<%= f.text_field :name, :placeholder => "Artist Name: " %>
<%= f.label :name,:autofocus => 'autofocus' %>
<br>
<%= f.text_field :song_name, :placeholder => "Song Name" %>
<%= f.label :song_name %>
<br>
<%= f.text_field :url, :placeholder => "You Tube URL" %>
<%= f.label :url %>
<br>
<%= f.text_field :post_by, :placeholder => "Post By" %>
<%= f.label :post_by %> <br>


<%= f.text_field :genre, :placeholder => "Post By" %>
<%= f.label :genre %>

<%= f.text_field :rating, :placeholder => "Post By" %>
<%= f.label :rating %>




<%= f.submit %>
  <% end %>


